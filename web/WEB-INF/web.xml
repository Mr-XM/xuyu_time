<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CopywriteServlet</servlet-name>
        <servlet-class>com.xuyu.servlet.CopywriteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CopywriteServlet</servlet-name>
        <url-pattern>/CopywriteServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateGoodAndSqlServlet</servlet-name>
        <servlet-class>com.xuyu.servlet.UpdateGoodAndSqlServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateGoodAndSqlServlet</servlet-name>
        <url-pattern>/UpdateGoodAndSqlServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WxOauthServlet</servlet-name>
        <servlet-class>com.xuyu.servlet.WxOauthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WxOauthServlet</servlet-name>
        <url-pattern>/WxOauthServlet</url-pattern>
    </servlet-mapping>


</web-app>